    "require": {
      "php": ">=7.0",
      "composer-plugin-api": "^2.1.0",
      "vlucas/phpdotenv": "^2.2"
    },
    "require-dev": {
      "composer/composer": ">=2.2.9"
    },
    "autoload": {
      "psr-4": {
        "Benignware\\BenignwareInstaller\\": "src"
      }
    },
    "extra": {
