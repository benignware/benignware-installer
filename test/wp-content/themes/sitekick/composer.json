{
  "name": "benignware/wp-sitekick",
  "version": "1.0.0-beta.2",
  "type": "wordpress-theme",
  "license": "UNLICENSED",
  "description": "Wordpress Kickstarter Theme",
  "homepage": "http://github.com/benignware/wp-sitekick",
  "authors": [
    {
      "name": "Rafael Nowrotek",
      "email": "mail@benignware.com",
      "homepage": "https://github.com/benignware"
    }
  ],
  "keywords": [
    "wordpress", "wordpress-theme", "bootstrap"
  ],
  "support": {
    "issues": "https://github.com/benignware/wp-sitekick/issues"
  },
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org"
  }, {
    "type": "package",
    "package": {
      "name": "benignware/alphabetic",
      "version": "v0.0.14",
      "type": "wordpress-plugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware-labs/wp-alphabetic.git",
        "reference": "v0.0.14"
      }
    }
  }, {
    "type": "package",
    "package": {
      "name": "benignware/bootstrap-hooks",
      "version": "v1.0.0-beta.4",
      "type": "wordpress-muplugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware/wp-bootstrap-hooks.git",
        "reference": "v1.0.0-beta.4"
      }
    }
  }, {
    "type": "package",
    "package": {
      "name": "benignware/basic-contact-form",
      "version": "v0.2.0-beta.12",
      "type": "wordpress-plugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware-labs/wp-basic-contact-form.git",
        "reference": "v0.2.0-beta.12"
      }
    }
  }, {
    "type": "package",
    "package": {
      "name": "benignware/menu-plus",
      "version": "v1.0.0-beta.2",
      "type": "wordpress-plugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware/wp-menu-plus.git",
        "reference": "v1.0.0-beta.2"
      }
    }
  }, {
    "type": "package",
    "package": {
      "name": "benignware/raudio",
      "version": "v1.0.0-beta.1",
      "type": "wordpress-plugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware/wp-raudio.git",
        "reference": "v1.0.0-beta.1"
      }
    }
  }, {
    "type": "package",
    "package": {
      "name": "benignware/responsive-thumbnail",
      "version": "v0.0.9",
      "type": "wordpress-plugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware-labs/wp-responsive-thumbnail.git",
        "reference": "v0.0.9"
      }
    }
  }, {
    "type": "package",
    "package": {
      "name": "benignware/swiper-shortcode",
      "version": "v0.1.0-beta.12",
      "type": "wordpress-plugin",
      "source": {
        "type": "git",
        "url": "https://github.com/benignware-labs/wp-swiper-shortcode.git",
        "reference": "v0.1.0-beta.12"
      }
    }
  }],
  "require": {
    "php": ">=5.3.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "wemakecustom/wp-mu-loader": "*",
    "benignware/alphabetic": ">=0.0.14",
    "benignware/bootstrap-hooks": ">=1.0.0-beta.4",
    "benignware/menu-plus": ">=1.0.0-beta.2",
    "benignware/raudio": ">=1.0.0-beta.1",
    "benignware/basic-contact-form": ">=0.2.0-beta.12",
    "benignware/responsive-thumbnail": ">=0.0.9",
    "benignware/swiper-shortcode": ">=0.1.0-beta.12",
    "wpackagist-plugin/advanced-custom-fields": ">=5.11.4",
    "wpackagist-plugin/author-avatars": ">=2.1.8",
    "wpackagist-plugin/custom-archives": ">=3.0.2",
    "wpackagist-plugin/custom-post-type-maker": ">=1.1.15",
    "wpackagist-plugin/disable-gutenberg": ">=2.6",
    "wpackagist-plugin/fakerpress": ">=0.5.2",
    "wpackagist-plugin/featured-video-plus": ">=2.3.3",
    "wpackagist-plugin/gallery-by-supsystic": ">=1.15.4",
    "wpackagist-plugin/groups": ">=2.16.1",
    "wpackagist-plugin/groups-bbpress": ">=1.3.0",
    "wpackagist-plugin/login-logout-menu": ">=1.3.1",
    "wpackagist-plugin/wp-mailhog-smtp": ">=1.0.1",
    "wpackagist-plugin/mailchimp-for-wp": ">=4.8.6",
    "wpackagist-plugin/post-type-archive-descriptions": ">=1.3.0",
    "wpackagist-plugin/post-type-archive-links": ">=1.3.1",
    "wpackagist-plugin/recent-posts-widget-extended": ">=0.9.9.7",
    "wpackagist-plugin/regenerate-thumbnails": ">=3.1.5",
    "wpackagist-plugin/bbpress": ">=2.6.9",
    "wpackagist-plugin/customizer-reset-by-wpzoom": ">=1.0.1",
    "wpackagist-plugin/menu-image": ">=3.0.5",
    "wpackagist-plugin/jetpack": ">=10.6", 
    "wpackagist-plugin/block-context": ">=0.1.0",
    "wpackagist-plugin/wordpress-seo": ">=9.7",
    "wpackagist-plugin/wordpress-importer": ">=0.6.4",
    "wpackagist-plugin/wp-mail-smtp":">=3.3.0",
    "wpackagist-plugin/wp-nav-plus": ">=3.4.9",
    "wpackagist-plugin/wp-user-profile-avatar": ">=1.0",
    "wpackagist-plugin/user-menus": ">=1.2.7",
    "wpackagist-plugin/fontmeister": ">=1.08",
    "wpackagist-plugin/woocommerce": ">=6.1.1",
    "wpackagist-plugin/otf-regenerate-thumbnails": ">=0.3",
    "wpackagist-plugin/font-awesome": ">=4.1.1",
    "wpackagist-plugin/thumbnail-editor": ">=2.3.2",
    "wpackagist-plugin/post-thumbnail-editor": ">=2.4.8",
    "wpackagist-plugin/display-environment-type": ">=1.3",
    "wpackagist-plugin/woo-order-test": ">=1.6",
    "wpackagist-plugin/woocommerce-gateway-paypal-express-checkout": ">=2.1.3",
    "wpackagist-theme/twentyseventeen": ">=2.9"
  },
  "scripts": {
    "post-autoload-dump": [
      "php -r \"copy('wp-content/mu-plugins/mu-loader/mu-require.php', 'wp-content/mu-plugins/mu-require.php');\""
    ]
  },
  "extra": {
    "webroot-dir": "wp-core",
  	"webroot-package": "wordpress",
    "installer-paths": {
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  }
}
